{
  "stores": {
    "CAS_MAIN_STORE": {
      "verify": {
        "backend": {
          "dedup": {
            "index_store": {
                  "filesystem": {
                    "content_path": "/tmp/bazel_cache/content_path-index",
                    "temp_path": "/tmp/bazel_cache/tmp_path-index",
                    "eviction_policy": {
                      // 32mb.
                      "max_bytes": 10000000000,
                    }
                  }
              // "fast_slow": {
              //   "fast": {
              //     "filesystem": {
              //       "content_path": "/tmp/bazel_cache/content_path-index",
              //       "temp_path": "/tmp/bazel_cache/tmp_path-index",
              //       "eviction_policy": {
