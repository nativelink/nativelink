{
  "stores": {
    "CAS_MAIN_STORE": {
      "verify": {
        "backend": {
          "dedup": {
            "index_store": {
              "fast_slow": {
                "fast": {
                  "filesystem": {
                    "content_path": "/tmp/bazel_cache/content_path-index",
                    "temp_path": "/tmp/bazel_cache/tmp_path-index",
                    "eviction_policy": {
                      // 100mb.
                      "max_bytes": 100000000,
                    }
                  }
                },
                "slow": {
                  "s3_store": {
                    "region": "us-west-1",
                    "bucket": "blaisebruer-cas-store",
                    "key_prefix": "test-prefix-index/",
                    "retry": {
                      "max_retries": 6,
                      "delay": 0.3,
                      "jitter": 0.5,
                    },
                    "additional_max_concurrent_requests": 10
                  }
                }
              }
            },
            "content_store": {
              "compression": {
                "compression_algorithm": {
                  "LZ4": {}
                },
                "backend": {
                  "fast_slow": {
                    "fast": {
                      "filesystem": {
                        "content_path": "/tmp/bazel_cache/content_path-content",
                        "temp_path": "/tmp/bazel_cache/tmp_path-content",
                        "eviction_policy": {
                          // 10gb.
                          "max_bytes": 10000000000,
                        }
                      }
                    },
                    "slow": {
                      "s3_store": {
                        "region": "us-west-1",
                        "bucket": "blaisebruer-cas-store",
                        "key_prefix": "test-prefix-dedup-cas/",
                        "retry": {
                          "max_retries": 6,
                          "delay": 0.3,
                          "jitter": 0.5,
                        },
                        "additional_max_concurrent_requests": 10
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "verify_size": true,
        "verify_hash": true
      }
    },
    "AC_MAIN_STORE": {
      "backend": {
        "fast_slow": {
          "fast": {
            // "memory": {
            //   "eviction_policy": {
            //     // 100mb.
            //     "max_bytes": 100000000,
            //   }
            // }
            "filesystem": {
              "content_path": "/tmp/bazel_cache/content_path-ac",
              "temp_path": "/tmp/bazel_cache/tmp_path-ac",
              "eviction_policy": {
                // 100mb.
                "max_bytes": 100000000,
              }
            }
          },
          "slow": {
            "s3_store": {
              "region": "us-west-1",
              "bucket": "blaisebruer-cas-store",
              "key_prefix": "test-prefix-ac/",
              "retry": {
                "max_retries": 6,
                "delay": 0.3,
                "jitter": 0.5,
              },
              "additional_max_concurrent_requests": 10
            }
          }
        }
      }
    }
  },
  "servers": [{
    "listen_address": "0.0.0.0:50051",
    "services": {
      "cas": {
        "main": {
          "cas_store": "CAS_MAIN_STORE"
        }
      },
      "ac": {
        "main": {
          "ac_store": "AC_MAIN_STORE"
        }
      },
      "capabilities": {},
      "execution": {},
      "bytestream": {
        "cas_stores": {
          "main": "CAS_MAIN_STORE",
        },
        // According to https://github.com/grpc/grpc.github.io/issues/371 16KiB - 64KiB is optimal.
        "max_bytes_per_stream": 64000, // 64kb.
      }
    }
  }]
}
